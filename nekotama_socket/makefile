OUT_DEBUG = bin/linux/Debug/libnekotama_socket.a

INC_RELEASE = $(INC) -I../nekotama_utils
CFLAGS_RELEASE = $(CFLAGS) -std=c++11 -O3 -Wfatal-errors -Wall
RESINC_RELEASE = $(RESINC)
RCFLAGS_RELEASE = $(RCFLAGS)
LIBDIR_RELEASE = $(LIBDIR)
LIB_RELEASE = $(LIB)../nekotama_utils/bin/linux/Release/libnekotama_utils.a
LDFLAGS_RELEASE = $(LDFLAGS) -s
OBJDIR_RELEASE = obj/linux/Release
DEP_RELEASE = 
OUT_RELEASE = bin/linux/Release/libnekotama_socket.a

OBJ_DEBUG = $(OBJDIR_DEBUG)/Socket_linux.o

OBJ_RELEASE = $(OBJDIR_RELEASE)/Socket_linux.o

all: debug release

clean: clean_debug clean_release

before_debug: 
	test -d bin/linux/Debug || mkdir -p bin/linux/Debug
	test -d $(OBJDIR_DEBUG) || mkdir -p $(OBJDIR_DEBUG)

after_debug: 

debug: before_debug out_debug after_debug

out_debug: before_debug $(OBJ_DEBUG) $(DEP_DEBUG)
	$(AR) rcs $(OUT_DEBUG) $(OBJ_DEBUG)

$(OBJDIR_DEBUG)/Socket_linux.o: Socket_linux.cpp
	$(CXX) $(CFLAGS_DEBUG) $(INC_DEBUG) -c Socket_linux.cpp -o $(OBJDIR_DEBUG)/Socket_linux.o

clean_debug: 
	rm -f $(OBJ_DEBUG) $(OUT_DEBUG)
	rm -rf bin/linux/Debug
	rm -rf $(OBJDIR_DEBUG)

before_release: 
	test -d bin/linux/Release || mkdir -p bin/linux/Release
	test -d $(OBJDIR_RELEASE) || mkdir -p $(OBJDIR_RELEASE)

after_release: 

release: before_release out_release after_release

out_release: before_release $(OBJ_RELEASE) $(DEP_RELEASE)
	$(AR) rcs $(OUT_RELEASE) $(OBJ_RELEASE)

$(OBJDIR_RELEASE)/Socket_linux.o: Socket_linux.cpp
	$(CXX) $(CFLAGS_RELEASE) $(INC_RELEASE) -c Socket_linux.cpp -o $(OBJDIR_RELEASE)/Socket_linux.o

clean_release: 

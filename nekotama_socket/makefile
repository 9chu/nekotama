CFLAGS_DEBUG = $(CFLAGS) -std=c++11 -Wfatal-errors -Wall -g
RESINC_DEBUG = $(RESINC)
RCFLAGS_DEBUG = $(RCFLAGS)
LIBDIR_DEBUG = $(LIBDIR)
LIB_DEBUG = $(LIB)../nekotama_utils/bin/linux/Debug/libnekotama_utils.a
LDFLAGS_DEBUG = $(LDFLAGS)
OBJDIR_DEBUG = obj/linux/Debug
